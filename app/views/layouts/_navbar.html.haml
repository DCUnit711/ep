.container
  .navbar-header
    %button.navbar-toggle{"data-target" => ".navbar-collapse", "data-toggle" => "collapse", :type => "button"}
      %span.icon-bar
      %span.icon-bar
      %span.icon-bar
    .navbar-brand
      = link_to 'Express Print', welcome_index_path
  .collapse.navbar-collapse
    - if user_signed_in?
      %ul.nav.navbar-nav
        %li= link_to 'Tickets', tickets_path
        %li= link_to 'Print Stations', print_stations_path
        %li= link_to 'Supplies', inventory_index_path
        %li= link_to 'Printers', printers_path
        %li= link_to 'Computers', computers_path
        %li= link_to 'Admin', admin_index_path
        %li= link_to 'Passdown', passdowns_path
    %ul.nav.navbar-nav.navbar-right
      - if user_signed_in?
        %li= link_to current_user.username, edit_user_registration_path
        %li= link_to 'Sign Out', destroy_user_session_path, method: 'delete'
      - else
        %li= link_to 'Sign In', new_user_session_path