.row
  .col-md-8
    %ul#myTab.nav.nav-tabs
      %li.active
        %a{"data-toggle" => "tab", :href => "#print_station"} Print Station Information
      %li
        %a{"data-toggle" => "tab", :href => "#ticket"} Ticket Info
  .col-md-4
    %h4
      Support Center:
      = copy_center(@print_station)
.row
  .col-md-12
    .tab-content
      #print_station.tab-pane.active
        %br
        =render 'info'
        %hr
        .row
          .col-md-9
            =render 'comments/list'
          =render 'comments/form'
      #ticket.tab-pane
        / =render 'tickets/list'
        %br
        =render 'tickets/open_tickets'
        =render 'tickets/resolved_tickets'
        =render 'tickets/form'